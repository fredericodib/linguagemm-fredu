Grammar

    0 $accept: prog $end

    1 prog: globalList

    2 globalList: globalList var
    3           | var
    4           | func
    5           | globalList func

    6 var: TYPEINT ID ';'
    7    | TYPEINT ID '=' INT ';'
    8    | TYPEFLOAT ID ';'
    9    | TYPEFLOAT ID '=' INT ';'
   10    | TYPEFLOAT ID '=' DEC ';'
   11    | TYPESTRING ID ';'
   12    | TYPESTRING ID '=' STR ';'

   13 $@1: %empty

   14 $@2: %empty

   15 func: TYPESTRING ID $@1 '(' paramsList ')' $@2 '{' contentList '}'

   16 $@3: %empty

   17 $@4: %empty

   18 func: TYPEFLOAT ID $@3 '(' paramsList ')' $@4 '{' contentList '}'

   19 $@5: %empty

   20 $@6: %empty

   21 func: TYPEINT ID $@5 '(' paramsList ')' $@6 '{' contentList '}'

   22 paramsList: params
   23           | %empty

   24 params: param
   25       | param ',' params

   26 param: TYPEINT ID
   27      | TYPEFLOAT ID
   28      | TYPESTRING ID

   29 contentList: content contentList
   30            | %empty

   31 content: var
   32        | addValue
   33        | comand
   34        | print
   35        | scan
   36        | return
   37        | callFunc

   38 addValue: ID '=' expression ';'

   39 expression: expression_mul_div
   40           | expression_mul_div op_add_sub expression

   41 expression_mul_div: ID
   42                   | INT
   43                   | DEC
   44                   | STR
   45                   | ID op_mul_div expression_mul_div
   46                   | INT op_mul_div expression_mul_div
   47                   | DEC op_mul_div expression_mul_div
   48                   | STR op_mul_div expression_mul_div

   49 op_add_sub: '+'
   50           | '-'

   51 op_mul_div: '/'
   52           | '*'

   53 comand: IF '(' condition_or ')' '{' contentList '}'
   54       | IF '(' condition_or ')' '{' contentList '}' ELSE '{' contentList '}'
   55       | WHILE '(' condition_or ')' '{' contentList '}'

   56 condition_or: condition_and
   57             | condition_and OR condition_or

   58 condition_and: condition
   59              | condition AND condition_and

   60 condition: ID cond ID
   61          | ID cond INT
   62          | ID cond DEC
   63          | INT cond ID
   64          | INT cond INT
   65          | INT cond DEC
   66          | DEC cond ID
   67          | DEC cond INT
   68          | DEC cond DEC

   69 cond: CGE
   70     | CGT
   71     | CLE
   72     | CLT
   73     | CNE
   74     | CEQ

   75 print: PRINT '(' ID ')' ';'
   76      | PRINT '(' STR ')' ';'
   77      | PRINT '(' INT ')' ';'
   78      | PRINT '(' DEC ')' ';'

   79 scan: SCAN '(' ID ',' TYPEINT ')' ';'
   80     | SCAN '(' ID ',' TYPEFLOAT ')' ';'
   81     | SCAN '(' ID ',' TYPESTRING ')' ';'

   82 return: RETURN '(' ID ')' ';'
   83       | RETURN '(' STR ')' ';'
   84       | RETURN '(' INT ')' ';'
   85       | RETURN '(' DEC ')' ';'

   86 callFunc: ID '(' callFuncParams ')' ';'
   87         | ID '=' ID '(' callFuncParams ')' ';'
   88         | ID '(' ')' ';'
   89         | ID '=' ID '(' ')' ';'

   90 callFuncParams: ID
   91               | INT
   92               | DEC
   93               | STR
   94               | callFuncParams ',' ID
   95               | callFuncParams ',' INT
   96               | callFuncParams ',' DEC
   97               | callFuncParams ',' STR


