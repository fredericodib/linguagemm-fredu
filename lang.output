Grammar

    0 $accept: prog $end

    1 prog: globalList

    2 globalList: globalList var
    3           | var
    4           | func
    5           | globalList func

    6 var: TYPEINT ID ';'
    7    | TYPEINT ID '=' INT ';'
    8    | TYPEFLOAT ID ';'
    9    | TYPEFLOAT ID '=' INT ';'
   10    | TYPEFLOAT ID '=' DEC ';'
   11    | TYPESTRING ID ';'
   12    | TYPESTRING ID '=' STR ';'

   13 $@1: %empty

   14 $@2: %empty

   15 func: TYPESTRING ID $@1 '(' paramsList ')' $@2 '{' contentList '}'

   16 $@3: %empty

   17 $@4: %empty

   18 func: TYPEFLOAT ID $@3 '(' paramsList ')' $@4 '{' contentList '}'

   19 $@5: %empty

   20 $@6: %empty

   21 func: TYPEINT ID $@5 '(' paramsList ')' $@6 '{' contentList '}'

   22 paramsList: params
   23           | %empty

   24 params: param
   25       | param ',' params

   26 param: TYPEINT ID
   27      | TYPEFLOAT ID
   28      | TYPESTRING ID

   29 contentList: content contentList
   30            | %empty

   31 content: var
   32        | addValue
   33        | comand
   34        | print
   35        | scan
   36        | return
   37        | callFunc

   38 addValue: ID '=' expression ';'

   39 expression: expression_mul_div
   40           | expression_mul_div op_add_sub expression

   41 expression_mul_div: ID
   42                   | INT
   43                   | DEC
   44                   | STR
   45                   | ID op_mul_div expression_mul_div
   46                   | INT op_mul_div expression_mul_div
   47                   | DEC op_mul_div expression_mul_div
   48                   | STR op_mul_div expression_mul_div

   49 op_add_sub: '+'
   50           | '-'

   51 op_mul_div: '/'
   52           | '*'

   53 comand: IF '(' condition_root ')' '{' contentList '}'

   54 $@7: %empty

   55 comand: IF '(' condition_root ')' '{' contentList '}' $@7 ELSE '{' contentList '}'

   56 $@8: %empty

   57 comand: WHILE $@8 '(' condition_root ')' '{' contentList '}'

   58 condition_root: condition_or

   59 condition_or: condition_and
   60             | condition_and OR condition_or

   61 condition_and: condition
   62              | condition AND condition_and

   63 condition: ID cond ID
   64          | ID cond INT
   65          | ID cond DEC
   66          | INT cond ID
   67          | INT cond INT
   68          | INT cond DEC
   69          | DEC cond ID
   70          | DEC cond INT
   71          | DEC cond DEC

   72 cond: CGE
   73     | CGT
   74     | CLE
   75     | CLT
   76     | CNE
   77     | CEQ

   78 print: PRINT '(' ID ')' ';'
   79      | PRINT '(' STR ')' ';'
   80      | PRINT '(' INT ')' ';'
   81      | PRINT '(' DEC ')' ';'

   82 scan: SCAN '(' ID ',' TYPEINT ')' ';'
   83     | SCAN '(' ID ',' TYPEFLOAT ')' ';'
   84     | SCAN '(' ID ',' TYPESTRING ')' ';'

   85 return: RETURN '(' ID ')' ';'
   86       | RETURN '(' STR ')' ';'
   87       | RETURN '(' INT ')' ';'
   88       | RETURN '(' DEC ')' ';'

   89 callFunc: ID '(' callFuncParams ')' ';'
   90         | ID '=' ID '(' callFuncParams ')' ';'
   91         | ID '(' ')' ';'
   92         | ID '=' ID '(' ')' ';'

   93 callFuncParams: ID
   94               | INT
   95               | DEC
   96               | STR
   97               | callFuncParams ',' ID
   98               | callFuncParams ',' INT
   99               | callFuncParams ',' DEC
  100               | callFuncParams ',' STR


