Grammar

    0 $accept: prog $end

    1 prog: globalList

    2 globalList: globalList var
    3           | var
    4           | func
    5           | globalList func

    6 var: TYPEINT ID ';'
    7    | TYPEINT ID '=' INT ';'
    8    | TYPEFLOAT ID ';'
    9    | TYPEFLOAT ID '=' INT ';'
   10    | TYPEFLOAT ID '=' DEC ';'
   11    | TYPESTRING ID ';'
   12    | TYPESTRING ID '=' STR ';'

   13 $@1: %empty

   14 $@2: %empty

   15 func: TYPESTRING ID $@1 '(' paramsList ')' $@2 '{' contentList '}'

   16 $@3: %empty

   17 $@4: %empty

   18 func: TYPEFLOAT ID $@3 '(' paramsList ')' $@4 '{' contentList '}'

   19 $@5: %empty

   20 $@6: %empty

   21 func: TYPEINT ID $@5 '(' paramsList ')' $@6 '{' contentList '}'

   22 paramsList: params
   23           | %empty

   24 params: param
   25       | param ',' params

   26 param: TYPEINT ID
   27      | TYPEFLOAT ID
   28      | TYPESTRING ID

   29 contentList: content contentList
   30            | %empty

   31 content: var
   32        | addValue
   33        | comand
   34        | print
   35        | scan
   36        | return
   37        | callFunc

   38 addValue: ID '=' expression ';'

   39 expression: ID
   40           | INT
   41           | DEC
   42           | STR
   43           | ID op expression
   44           | INT op expression
   45           | DEC op expression
   46           | STR op expression

   47 op: '+'
   48   | '-'
   49   | '/'
   50   | '*'

   51 comand: IF '(' condition ')' '{' contentList '}'
   52       | IF '(' condition ')' '{' contentList '}' ELSE '{' contentList '}'
   53       | WHILE '(' condition ')' '{' contentList '}'

   54 condition: ID cond ID
   55          | ID cond INT
   56          | ID cond DEC
   57          | INT cond ID
   58          | INT cond INT
   59          | INT cond DEC
   60          | DEC cond ID
   61          | DEC cond INT
   62          | DEC cond DEC

   63 cond: CGE
   64     | CGT
   65     | CLE
   66     | CLT
   67     | CNE
   68     | CEQ

   69 print: PRINT '(' ID ')' ';'
   70      | PRINT '(' STR ')' ';'
   71      | PRINT '(' INT ')' ';'
   72      | PRINT '(' DEC ')' ';'

   73 scan: SCAN '(' ID ',' TYPEINT ')' ';'
   74     | SCAN '(' ID ',' TYPEFLOAT ')' ';'
   75     | SCAN '(' ID ',' TYPESTRING ')' ';'

   76 return: RETURN '(' ID ')' ';'
   77       | RETURN '(' STR ')' ';'
   78       | RETURN '(' INT ')' ';'
   79       | RETURN '(' DEC ')' ';'

   80 callFunc: ID '(' callFuncParams ')' ';'
   81         | ID '=' ID '(' callFuncParams ')' ';'
   82         | ID '(' ')' ';'
   83         | ID '=' ID '(' ')' ';'

   84 callFuncParams: ID
   85               | INT
   86               | DEC
   87               | STR
   88               | ID ',' callFuncParams
   89               | INT ',' callFuncParams
   90               | DEC ',' callFuncParams
   91               | STR ',' callFuncParams


